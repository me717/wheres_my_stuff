package edu.gatech.num1matches.model;


/**
 * An information holder class that contains contact information about a user.
 * @author Andrew Dehn
 *
 */
public class ContactInfo 
{
	public String name;
	public String phoneNumber;
	public String emailAddress; // Overrode UML class diagram, no logical reason at this stage to have this be private. -Andrew
	public String streetAddress;
	public String city;
	public String state;
	public String zipCode;
	
	/**
	 * Creates the contact info object
	 * @param name the name of the user
	 * @param emailAddress the user's email
	 */
	public ContactInfo(String name, String emailAddress)
	{
		this.name = name;
		this.emailAddress = emailAddress;
		this.phoneNumber = "";
		this.streetAddress = "";
		this.city = "";
		this.state = "";
		this.zipCode = "";
	}
	
	/**
	 * A more detailed constructor
	 * @param name the name of the user
	 * @param emailAddress the user's email
	 * @param phoneNumber the user's phone number
	 * @param streetAddress the user's adress
	 * @param city the city the user lives in
	 * @param state the state the user lives in
	 * @param zipCode the user's zip code
	 */
	public ContactInfo(String name, String emailAddress, String phoneNumber, String streetAddress, String city, String state, String zipCode)
	{
		this.name = name;
		this.emailAddress = emailAddress;
		this.phoneNumber = phoneNumber;
		this.streetAddress = streetAddress;
		this.city = city;
		this.state = state;
		this.zipCode = zipCode;
	}
	
}
